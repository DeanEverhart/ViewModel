@model IEnumerable<ViewModel.Models.A>

@{
    ViewData["A"] = "A";
}

<h1>A</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.One)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Two)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Three)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.One)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Two)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Three)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
